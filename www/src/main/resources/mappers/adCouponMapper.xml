<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.booktopia.www.repository.CouponUseMapper">
    <select id="getCount" resultType="int">
        select count(cou_no) from ad_coupon
    </select>

    <select id="getList" resultType="com.booktopia.www.domain.AdCouponVO">
        select * from ad_coupon
        order by ad_cou_date desc
        limit #{pageStart},#{qty}
    </select>
</mapper>